/**app.wxss**/
@import 'weui.wxss';
.w__100 {
  width: 100%;
}
.disp__inline {
  display: inline;
  color: #BAA68B
}
.container {
  height: 100%;
  width: 100%;
  position: relative;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  box-sizing: border-box;
}
.icon__arrow__right {
  width: 20rpx;
  height: 20rpx;
  margin-left: 18rpx;
  display: inline-flex;
  background: url('data:image/png;base64,') no-repeat;
  background-size: 100% 100%;
}
.icon__heart {
  width: 28rpx;
  height: 28rpx;
  margin-left: 10rpx;
  display: inline-flex;
  background: url('data:image/png;base64,') no-repeat;
  background-size: 100% 100%;
}
.icon__home {
  width: 30rpx;
  height: 30rpx;
  margin-right: 10rpx;
  display: inline-flex;
  background: url('data:image/png;base64,') no-repeat;
  background-size: 100% 100%;
}
.icon__share {
  width: 30rpx;
  height: 30rpx;
  margin-right: 10rpx;
  display: inline-flex;
  background: url('data:image/png;base64,') no-repeat;
  background-size: 100% 100%;
}
.wx-swiper-dot-active{
  width:32rpx!important;
  border-radius:50rpx!important;
}