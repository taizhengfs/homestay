/**app.wxss**/
@import 'weui.wxss';
.w__100 {
  width: 100%;
}
.disp__inline {
  display: inline;
  color: #BAA68B
}
image {
  width: 100%;
  height: 100%;
  position: absolute;
  left: 0;
  top: 0;
}
.disp__inline__red {
  display: inline;
  color: #C12121
}
.container {
  height: 100%;
  width: 100%;
  position: relative;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  box-sizing: border-box;
}
.icon__arrow__right {
  width: 20rpx;
  height: 20rpx;
  margin-left: 18rpx;
