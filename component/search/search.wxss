.home__search input {
  width: 390rpx;
  line-height: 72rpx;
  height: 72rpx;
  font-size: 28rpx!important;
  padding-left: 80rpx;
  position: relative;
}
.home__search__content {
  background: rgba(255,255,255,0.85);
  width: 100%;
  height: 72rpx;
  border-radius: 20rpx;
  box-sizing: border-box;
  border: none;
  display: flex;
  align-items: center;
  justify-content: space-between;
  position: relative;
  box-shadow: 0 4rpx 16rpx 0 RGBA(201, 202, 202, 0.37); 
}
.home__search {
  width: 100%;
  padding: 0 34rpx;
  height: 72rpx; 
  box-sizing: border-box;
  position: relative;
  z-index: 2;
}
.search_icon {
  width: 36rpx!important;
  height: 37rpx!important;
  position: absolute;
  left: 28rpx;
  top: 18rpx;
}
.btn_search {
  color: #BAA68B;
  padding: 0 30rpx 0 38rpx;
  font-size: 32rpx;
  position: absolute;
  height: 52rpx;
  line-height: 52rpx;
  letter-spacing: 10rpx;
  text-align: center;
  border-left: 2rpx solid #EDEDED;
  right: 0;
  top: 10rpx;
  z-index: 20;
}
.search_delet {
  width: 34rpx!important;
  height: 34rpx!important;
  padding: 19rpx;
  position: absolute;
  z-index: 10;  
  right: 148rpx;
}
