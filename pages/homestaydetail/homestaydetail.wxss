/* pages/homestaydetail/homestaydetail.wxss */
.suji__tags{
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
  flex-wrap: wrap;
  width: 100%
}
.suji__tag {
  color: #9F8A60;
  font-size: 24rpx;
  line-height: 34rpx;
  padding: 3rpx 16rpx;
  border-radius: 6rpx;
  margin-right: 20rpx;
  margin-bottom: 20rpx;
  border:2rpx solid rgba(186,166,139,1);
}
.detail__tags {
  padding: 28rpx 34rpx 8rpx 34rpx;
  box-sizing: border-box;
  border-bottom: 2rpx solid #EAEAEA;
  width: 100%;
  position: relative;
}
.detail__cell {
  width: 100%;
  min-height: 100rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  border-bottom:2rpx solid rgba(234,234,234,1);
}
.detail__cell.phone {
  height: 70rpx;
  border-bottom: none;
}
.detail__cell__icon {
  font-size: 28rpx;
  line-height: 40rpx;
  color: #333;
  display: flex;
  justify-content: flex-start;
  flex-direction: row;
  padding: 10rpx 0;
  align-items: flex-start
}
.detail__cell__icon image {
  width: 26rpx;
  min-width: 26rpx;
  height: 26rpx;
  margin-right: 18rpx;
  margin-top: 7rpx;
}
.detail__cell__right {
  display: flex;
  align-items: center;
  flex-direction: row;
  color: #BAA68B;
  font-size: 24rpx;
  min-width: 100rpx;
  line-height: 34rpx;
}
.detail__location {
  width: 100%;
  box-sizing: border-box;
  padding:0 24rpx;
  border-bottom: 2rpx solid #EAEAEA;
}
.detail__cell__display {
  display: flex;
  flex-direction: column;
}
.detail__cell__display__top {
  font-size: 28rpx;
  line-height: 40rpx;
  color: #333;
}
.detail__cell__display__bottom {
  font-size: 24rpx;
  line-height: 34rpx;
  color: #7f7f7f;
}
.detail__phone {
  color: #333;
}
.detail__ticket__block {
  width: 100rpx;
  height: 100rpx;
  position: relative;
  display: flex;
  flex-direction: column;
  justify-content: center;
  background: #f2f0ec;
  border-radius: 6rpx;
}
.detail__ticket__top {
  display: flex;
  justify-content: center;
  width: 100%;
  height: 66rpx;
  align-items: center;
}
.detail__ticket__top image {
  width: 43rpx;
  height: 43rpx;
}
.detail__ticket__bottom {
  width: 100%;
  height: 34rpx;
  text-align: center;
  background: #BAA68B;
  color: #fff;
  line-height: 34rpx;
  font-size: 24rpx;
  line-height: 34rpx;
  border-bottom-right-radius: 6rpx;
  border-bottom-left-radius: 6rpx;
}
.detail__ticket__maim {
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  padding-right: 40rpx;
  min-width: 150rpx;
}
.detail__ticket__title {
  margin-top: 18rpx;
  text-align: center;
  font-size: 24rpx;
  line-height: 34rpx;
  color: #333;
}
.detail__ex__block_title {
  color: #333;
  font-size: 28rpx;
  line-height: 40rpx;
}
.detail__ex__block_part {
  color: #858585;
  font-size: 20rpx;
  line-height: 28rpx;
}
.detail__experience {
  width: 100%;
  padding: 0 22rpx;
  box-sizing: border-box;
  border-bottom: 2rpx solid #EAEAEA;
}
.detail__ex__block {
  display: flex;
  width: 100%;
  position: relative;
  flex-direction: row;
  align-items: center;
  padding: 26rpx 0;
  border-bottom: 2rpx solid #EAEAEA;
}
.detail__ex__block:last-child {
  border-bottom: none;
}
.detail__ex__block_c {
  display: flex;
  flex: 1;
  flex-direction: column;
  justify-content: flex-start;
  padding-left: 22rpx;
}
.detail__join {
  color: #fff;
  padding: 4rpx 30rpx;
  border-radius: 6rpx;
  font-size: 24rpx;
  line-height: 34rpx;
  background: #C12121
}
.detail__card_pad {
  padding: 20rpx 45rpx;
}
.detail__card__desc {
  width: 100%;
  box-sizing: border-box;
  font-size: 24rpx;
  line-height: 40rpx;
  color: #333;
}
.detail__card__roomlist {
  width: 100%;
  position: relative;
  box-sizing: border-box;
}
.room__list {
  width: 100%;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.room__list__l {
  display: flex;
  flex-direction: column;
  justify-content: flex-start;
}
.room__list__r {
  display: flex;
  flex-direction: column;
  justify-content: flex-end;
}
.room__list__r__t {
  color: #868686;
  font-size: 24rpx;
  line-height: 34rpx;
}
.room__list__l__t {
  color: #333;
  font-size: 28rpx;
  line-height: 40rpx;
}
.room__list__r__b {
  color: #C12121;
  font-size: 36rpx;
  line-height: 50rpx;
}
.room__list__l__m {
  color: #868686;
  font-size: 20rpx;
  line-height: 28rpx;
}
.room__list__l__b {
  color: #BAA68B;
  font-size: 20rpx;
  line-height: 28rpx;
}
.detail__card__support {
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
  justify-content: flex-start;
}
.support__single {
  font-size: 24rpx;
  line-height: 48rpx;
  color: #868686;
  padding-right: 20rpx;
}
.support__single.cls__gold {
  color: #BAA68B
}
.detail__info__block {
  font-size:24rpx;
  font-family:PingFangSC-Light;
  line-height:44rpx;
}
.detail__info__dishes {
  width: 100vw;
  height: 150rpx;
  position: relative;
}
.detail__disher__content {
  height: 150rpx;
  overflow-x: scroll;
  overflow-y: hidden;
  display: flex;
  flex-direction: row;
  flex-wrap: nowrap;
  padding-left: 24rpx;
}
.detail__disher__block {
  min-width: 200rpx;
  height: 150rpx;
  display: flex;
  flex-direction: column;
  justify-content: center;
  margin-right: 24rpx;
}
.detail__disher__block_t {
  width: 100%;
  height: 120rpx;
  border-radius: 6rpx;
  overflow: hidden; 
  position: relative;
}
.detail__disher__block_t image {
  width: 100%;
  position: absolute;
  left: 50%;
  top: 50%;
  transform: translate(-50%, -50%);
  border-radius: 6rpx;
}
.detail__disher__block_b {
  color: #333;
  font-size: 24rpx;
  line-height: 34rpx;
  text-align: center;
  margin-top: 10rpx;
}