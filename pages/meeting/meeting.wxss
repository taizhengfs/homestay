/* pages/meeting/meeting.wxss */
page {
  background: #F4F4F4;
}
.meeting__block {
  width: 100%;
  height: 160rpx;
  display: flex;
  flex-direction: row;
  background: #fff;
  margin-top: 40rpx;
  align-items: center;
}
.meeting__left {
  width: 108rpx;
  height: 108rpx;
  position: relative;
  margin-left: 36rpx;
}
.meeting__left image {
  width: 100%;
  height: 100%;
  position: absolute;
}
.meeting__right__top {
  font-size:28rpx;
  color:rgba(51,51,51,1);
  line-height:40rpx;
}
.meeting__right {
  padding-left: 24rpx;
}
.meeting__right__middle {
  font-size:20rpx;
  color:rgba(153,153,153,1);
  line-height:28rpx;
  margin-top: 4rpx;
}
.meeting__right__bottom {
  font-size:20rpx;
  color:rgba(186,166,139,1);
  line-height:28rpx;
  margin-top: 12rpx;
}